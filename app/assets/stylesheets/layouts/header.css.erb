.header { width: 100%; justify-content: space-around }

.header-img-wrapper { margin: 0; padding: 20px 0 6px 0 }

.header-img {
	width: 110px;
  height: 110px;
  background-size: 360px;
  background-image: url(<%= asset_path('food_pics.jpg') %>);
  border-radius: 60px;
}

#img-burger { background-position: -235px -14px }

#img-eggs { background-position: -125px 134px }

#img-pizza { background-position: -125px -14px }

#img-sweets { background-position: -15px -14px }

@media only screen and (max-width: 425px) {
  .header-img { width: 60px; height: 60px; background-size: 200px }
  
  #img-burger { background-position: -130px -8px }
  
  #img-eggs { background-position: -68px -127px }
  
  #img-pizza { background-position: -70px -8px }
  
  #img-sweets { background-position: -210px -8px }
}